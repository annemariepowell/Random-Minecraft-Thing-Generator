{
	"name": "Random Minecraft Item Generator",
	"version": "1.0",
	"description": "Randomly Generates Minecraft Item",
	"manifest_version": 2,
	"icons": {
		"27": "images/logo.png"
	},
	"background": {
		"persistent": true,
		"scripts":["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Generate Random Item!"
	},
	"permissions": [
		"storage",
		"tabs",
		"<all_urls>"
	]
}
